<div class="row g-3">
   <% news.forEach(element => { %>
        <div class="col-4 col-xl-4 col-lg-4 col-md-6 col-sm-6 col-xs-12">
            <div class="news__wrapper--item">
                <a href="" class="news__item--img">
                    <img src=<%= element.img %> alt="">
                </a>
                <p><%= element.content %></p>
            </div>
        </div>
   <% }) %>
</div>